{"ast":null,"code":"import { getQRCode, getCode, getQRresult } from \"@/request/api/home.js\";\nimport axios from \"axios\";\nimport { response } from \"express\";\nexport default {\n  data() {\n    return {\n      QRcodeImg: \"\",\n      qrCodeResult: null,\n      unikey: \"\",\n      pollingInterval: null\n    };\n  },\n  methods: {\n    QRCode: async function () {\n      let code = await getCode();\n      let result = await getQRCode(code);\n      this.QRcodeImg = result.data.data.qrimg;\n      this.unikey = code.data.data.unikey;\n    },\n    startPolling: async function () {\n      this.stopPolling();\n      this.pollingInterval = setInterval(async () => {\n        // 把async函数放在这里\n        try {\n          const response = await getQRresult(this.unikey);\n          if (response.data.code === 803) {\n            this.stopPolling();\n            alert(\"登录成功\");\n          }\n        } catch (error) {\n          console.error(error);\n        }\n      }, 100);\n    },\n    stopPolling() {\n      clearInterval(this.pollingInterval);\n    }\n  },\n  watch: {\n    unikey() {\n      this.startPolling();\n    }\n  }\n};","map":{"version":3,"names":["getQRCode","getCode","getQRresult","axios","response","data","QRcodeImg","qrCodeResult","unikey","pollingInterval","methods","QRCode","code","result","qrimg","startPolling","stopPolling","setInterval","alert","error","console","clearInterval","watch"],"sources":["D:\\java项目\\网易云音乐\\wangyiyun-app\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div @click=\"QRCode\">登录</div>\r\n    <div>\r\n      <img :src=\"QRcodeImg\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { getQRCode, getCode, getQRresult } from \"@/request/api/home.js\";\r\nimport axios from \"axios\";\r\nimport { response } from \"express\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      QRcodeImg: \"\",\r\n      qrCodeResult: null,\r\n      unikey: \"\",\r\n      pollingInterval: null,\r\n    };\r\n  },\r