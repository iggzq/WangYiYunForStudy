{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Cookies from \"js-cookie\";\nimport { getUserInfo, getLikeList, getMusicUrl, getHeartLoveModel, getUserDetail } from \"@/request/api/home.js\";\nexport default {\n  name: \"myInfomation\",\n  created: async function () {\n    if (Cookies.get(\"user\") === null) {\n      console.log(\"未登录\");\n      this.$router.push(\"login\");\n    } else {\n      this.$router.push(\"login\");\n      this.userInfo = await getUserInfo();\n      this.likeMusicList = await getLikeList(this.userInfo.data.profile.userId);\n      console.log(this.likeMusicList);\n      this.firstLikeMusic = await getMusicUrl(this.likeMusicList.data.ids[0]);\n    }\n  },\n  data() {\n    return {\n      userInfo: null,\n      likeMusicList: [],\n      firstLikeMusic: null\n    };\n  },\n  methods: {\n    goLikeMusicList: function () {\n      this.$router.push({\n        path: \"/likeMusicList\",\n        query: {\n          firstLikeMusic: JSON.stringify(this.firstLikeMusic),\n          itemLikeList: JSON.stringify(this.likeMusicList)\n        }\n      });\n    },\n    goPlayLoveModel: async function () {\n      let result1 = await getUserDetail();\n      let user = Cookies.get(\"user\");\n      console.log(user);\n      console.log(result1);\n    }\n  }\n};","map":{"version":3,"names":["Cookies","getUserInfo","getLikeList","getMusicUrl","getHeartLoveModel","getUserDetail","name","created","get","console","log","$router","push","userInfo","likeMusicList","data","profile","userId","firstLikeMusic","ids","methods","goLikeMusicList","path","query","JSON","stringify","itemLikeList","goPlayLoveModel","result1","user"],"sources":["D:\\java项目\\网易云音乐\\wangyiyun-app\\src\\views\\MyInfomation.vue"],"sourcesContent":["<template>\r\n  <div class=\"main\" v-if=\"userInfo && likeMusicList.length != 0 && firstLikeMusic != null\">\r\n    <div class=\"userIcon\">\r\n      <img :src=\"userInfo.data.profile.avatarUrl\" />\r\n    </div>\r\n    <div class=\"iconBorder\">\r\n      <span class=\"userName\">{{ userInfo.data.profile.nickname }}</span>\r\n    </div>\r\n\r\n    <div class=\"likeMusicListDiv\" @click=\"goLikeMusicList()\">\r\n      <div class=\"leftLikeMusicDiv\">\r\n        <!-- <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-xihuan3\"></use>\r\n        </svg> -->\r\n        <img :src=\"firstLikeMusic.data.songs[0].al.picUrl\" />\r\n        <svg class=\"bofang\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-xihuan3\"></use>\r\n        </svg>\r\n        <span class=\"likeMusicText\">我喜欢的音乐</span>\r\n        <span class=\"likeMusicLength\"\r\n          >{{ this.likeMusicList.data.ids.length }}首</span\r\n        >\r\n      </div>\r\n      <div class=\"rightMusicListDiv\" @click=\"goPlayLoveModel\">\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-icon_xindong\"></use>\r\n        </svg>\r\n        <span>心动模式</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Cookies from \"js-cookie\";\r\nimport { getUserInfo, getLikeList, getMusicUrl,getHeartLoveModel,getUserDetail } from \"@/request/api/home.js\";\r\nexport default {\r\n  name: \"myInfomation\",\r\n  created: async function () {\r\n    if (Cookies.get(\"user\") === null) {\r\n      console.log(\"未登录\");\r\n      this.$router.push(\"login\");\r\n    } else {\r\n      this.$router.push(\"login\");\r\n      this.userInfo = await getUserInfo();\r\n      this.likeMusicList = await getLikeList(this.userInfo.data.profile.userId);\r\n      console.log(this.likeMusicList);\r\n      this.firstLikeMusic = await getMusicUrl(this.likeMusicList.data.ids[0]);\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      userInfo: null,\r\n      likeMusicList: [],\r\n      firstLikeMusic: null,\r\n    };\r\n  },\r\n  methods: {\r\n    goLikeMusicList: function () {\r\n      this.$router.push({\r\n        path: \"/likeMusicList\",\r\n        query:{\r\n          firstLikeMusic: JSON.stringify(this.firstLikeMusic),\r\n          itemLikeList: JSON.stringify(this.likeMusicList),\r\n        }\r\n      });\r\n    },\r\n    goPlayLoveModel:async function(){\r\n      let result1 = await getUserDetail();\r\n      let user = Cookies.get(\"user\");\r\n      console.log(user);\r\n      console.log(result1);\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.main {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 93vh;\r\n  .userIcon {\r\n    position: absolute;\r\n    top: 6%;\r\n    left: 37%;\r\n    img {\r\n      position: absolute;\r\n      width: 2rem;\r\n      height: 2rem;\r\n      border-radius: 50%;\r\n      z-index: 1  ;\r\n    }\r\n  }\r\n  .iconBorder {\r\n    position: absolute;\r\n    z-index: 0;\r\n    top: 13%;\r\n    left: 5%;\r\n    border-radius: 20px;\r\n    border: 2px solid #999;\r\n    width: 90%;\r\n    height: 10%;\r\n    background-color: #999;\r\n    .userName {\r\n      position: absolute;\r\n      top: 60%;\r\n      left: 45%;\r\n    }\r\n  }\r\n  .likeMusicListDiv {\r\n    position: absolute;\r\n    z-index: 0;\r\n    top: 26%;\r\n    left: 5%;\r\n    border-radius: 20px;\r\n    border: 2px solid #999;\r\n    width: 90%;\r\n    height: 10%;\r\n    .leftLikeMusicDiv {\r\n      img {\r\n        position: absolute;\r\n        top: 10%;\r\n        left: 4%;\r\n        width: 1.2rem;\r\n        height: 1.2rem;\r\n        border-radius: 50%;\r\n        filter: blur(2px);\r\n      }\r\n      svg{\r\n        position: absolute;\r\n        left: 7%;\r\n        top: 23%;\r\n        width: 0.78rem;\r\n        height: 0.78rem;\r\n      }\r\n      .likeMusicText {\r\n        position: absolute;\r\n        top: 27%;\r\n        left: 23%;\r\n        font-weight: 600;\r\n      }\r\n      .likeMusicLength {\r\n        position: absolute;\r\n        bottom: 25%;\r\n        left: 23%;\r\n        font-size: xx-small;\r\n        color: #999;\r\n      }\r\n    }\r\n\r\n    .rightMusicListDiv {\r\n      border: 1px solid #999;\r\n      width: 1.5rem;\r\n      height: 0.6rem;\r\n      position: absolute;\r\n      right: 7%;\r\n      top: 30%;\r\n      border-radius: 100px;\r\n      display: flex;\r\n      justify-content: space-around;\r\n      align-items: center;\r\n      svg {\r\n        width: 0.5rem;\r\n        height: 0.5rem;\r\n      }\r\n      font-size: xx-small;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAiCA,OAAOA,OAAM,MAAO,WAAW;AAC/B,SAASC,WAAW,EAAEC,WAAW,EAAEC,WAAW,EAACC,iBAAiB,EAACC,aAAY,QAAS,uBAAuB;AAC7G,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,OAAO,EAAE,eAAAA,CAAA,EAAkB;IACzB,IAAIP,OAAO,CAACQ,GAAG,CAAC,MAAM,MAAM,IAAI,EAAE;MAChCC,OAAO,CAACC,GAAG,CAAC,KAAK,CAAC;MAClB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;IAC5B,OAAO;MACL,IAAI,CAACD,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;MAC1B,IAAI,CAACC,QAAO,GAAI,MAAMZ,WAAW,EAAE;MACnC,IAAI,CAACa,aAAY,GAAI,MAAMZ,WAAW,CAAC,IAAI,CAACW,QAAQ,CAACE,IAAI,CAACC,OAAO,CAACC,MAAM,CAAC;MACzER,OAAO,CAACC,GAAG,CAAC,IAAI,CAACI,aAAa,CAAC;MAC/B,IAAI,CAACI,cAAa,GAAI,MAAMf,WAAW,CAAC,IAAI,CAACW,aAAa,CAACC,IAAI,CAACI,GAAG,CAAC,CAAC,CAAC,CAAC;IACzE;EACF,CAAC;EACDJ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,EAAE;MACjBI,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,eAAe,EAAE,SAAAA,CAAA,EAAY;MAC3B,IAAI,CAACV,OAAO,CAACC,IAAI,CAAC;QAChBU,IAAI,EAAE,gBAAgB;QACtBC,KAAK,EAAC;UACJL,cAAc,EAAEM,IAAI,CAACC,SAAS,CAAC,IAAI,CAACP,cAAc,CAAC;UACnDQ,YAAY,EAAEF,IAAI,CAACC,SAAS,CAAC,IAAI,CAACX,aAAa;QACjD;MACF,CAAC,CAAC;IACJ,CAAC;IACDa,eAAe,EAAC,eAAAA,CAAA,EAAgB;MAC9B,IAAIC,OAAM,GAAI,MAAMvB,aAAa,EAAE;MACnC,IAAIwB,IAAG,GAAI7B,OAAO,CAACQ,GAAG,CAAC,MAAM,CAAC;MAC9BC,OAAO,CAACC,GAAG,CAACmB,IAAI,CAAC;MACjBpB,OAAO,CAACC,GAAG,CAACkB,OAAO,CAAC;IACtB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}