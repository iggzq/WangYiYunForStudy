{"ast":null,"code":"import { Vue3Marquee } from \"vue3-marquee\";\nimport \"vue3-marquee/dist/style.css\";\nexport default {\n  name: \"MusicDetail\",\n  props: [\"musicList\", \"isbtnShow\", \"changeDetailShow\", \"play\", \"detailShow\", \"lyricList\"],\n  mounted() {\n    console.log(this.musicList);\n  },\n  components: {\n    Vue3Marquee\n  },\n  data() {\n    return {\n      isLyricShow: false\n    };\n  },\n  computed: {\n    lyric: function () {\n      let arr;\n      if (this.lyricList.lrc.lyric) {\n        arr = this.lyricList.lrc.lyric.split(/[(\\r\\n)\\r\\n]+/).map((item, i) => {\n          let min = item.slice(1, 3);\n          let sec = item.slice(4, 6);\n          let mill = item.slice(item.indexOf('.') + 1, item.indexOf(']'));\n          let lrc = item.slice(11, item.length);\n          console.log(min, sec, mill, lrc);\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["Vue3Marquee","name","props","mounted","console","log","musicList","components","data","isLyricShow","computed","lyric","arr","lyricList","lrc","split","map","item","i","min","slice","sec","mill","indexOf","length"],"sources":["D:\\java项目\\网易云音乐\\wangyiyun-app\\src\\components\\item\\MusicDetail.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <img :src=\"musicList.al.picUrl\" alt=\"\" class=\"bgImg\" />\r\n    <div class=\"detailTop\">\r\n      <div class=\"detailTopLeft\">\r\n        <svg class=\"icon\" aria-hidden=\"true\" @click.stop=\"changeDetailShow\">\r\n          <use xlink:href=\"#icon-fanhui\"></use>\r\n        </svg>\r\n        <div class=\"leftMarquee\">\r\n          <Vue3Marquee style=\"color: #fff\">{{ musicList.name }}</Vue3Marquee>\r\n          <span v-for=\"item in musicList.ar\" :key=\"item\">\r\n            {{ item.name }}\r\n          </span>\r\n          <svg class=\"icon\" aria-hidden=\"true\">\r\n            <use xlink:href=\"#icon-fanhui\"></use>\r\n          </svg>\r\n        </div>\r\n      </div>\r\n      <div class=\"detailRight\">\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-fenxiang\"></use>\r\n        </svg>\r\n      </div>\r\n    </div>\r\n    <div class=\"detailContent\" v-show=\"isLyricShow\">\r\n      <img src=\"@/assets/needle-ab.png\" alt=\"\" class=\"img_needle\" :class=\"{img_needle_active:!isbtnShow}\"/>\r\n      <img src=\"@/assets/musicBackground.png\" class=\"img_cd\" />\r\n      <img :src=\"musicList.al.picUrl\" class=\"img_ar\" :class=\"{img_ar_active:!isbtnShow,img_ar_paused:isbtnShow}\">\r\n    </div>\r\n    <div class=\"musicLyric\">\r\n      {{ lyricList.lrc.lyric }}\r\n      {{ lyric }}\r\n    </div>\r\n    <div class=\"detailFooter\">\r\n      <div class=\"footerTop\">\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-xihuan\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-xiazai1\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-yinle\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-pinglun1\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-31liebiao\"></use>\r\n        </svg>\r\n      </div>\r\n      <div class=\"footerMid\"></div>\r\n      <div class=\"footerFoot\">\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-fanhui\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-48shangyishou\"></use>\r\n        </svg>\r\n        <svg class=\"bofang\" aria-hidden=\"true\" v-if=\"isbtnShow\" @click.stop=\"play\">\r\n          <use xlink:href=\"#icon-bofang\"></use>\r\n        </svg>\r\n        <svg class=\"bofang\" aria-hidden=\"true\" v-else @click.stop=\"play\">\r\n          <use xlink:href=\"#icon-zanting\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-a-49xiayishou\"></use>\r\n        </svg>\r\n        <svg class=\"icon\" aria-hidden=\"true\">\r\n          <use xlink:href=\"#icon-danquxunhuan\"></use>\r\n        </svg>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Vue3Marquee } from \"vue3-marquee\";\r\nimport \"vue3-marquee/dist/style.css\";\r\nexport default {\r\n  name: \"MusicDetail\",\r\n  props: [\"musicList\",\"isbtnShow\",\"changeDetailShow\",\"play\",\"detailShow\",\"lyricList\"],\r\n  mounted() {\r\n    console.log(this.musicList);\r\n  },\r\n  components: {\r\n    Vue3Marquee,\r\n  },\r\n  data(){\r\n    return{\r\n      isLyricShow:false\r\n    }\r\n  },\r\n  computed:{\r\n    lyric:function(){\r\n      let arr;\r\n      if(this.lyricList.lrc.lyric){\r\n        arr = this.lyricList.lrc.lyric.split(/[(\\r\\n)\\r\\n]+/).map((item,i)=>{\r\n          let min = item.slice(1,3);\r\n          let sec = item.slice(4,6);\r\n          let mill = item.slice(item.indexOf('.')+1,item.indexOf(']'));\r\n          let lrc = item.slice(11,item.length);\r\n          console.log(min,sec,mill,lrc);\r\n        })\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.detailTop {\r\n  width: 100%;\r\n  height: 1rem;\r\n  display: flex;\r\n  font-size: small;\r\n  justify-content: space-between;\r\n  padding: 0.2rem;\r\n  align-items: center;\r\n  fill: #fff;\r\n  .detailTopLeft {\r\n    display: flex;\r\n    align-items: center;\r\n    .leftMarquee {\r\n      width: 3rem;\r\n      height: 100%;\r\n      margin-left: 0.4rem;\r\n      span {\r\n        color: #999;\r\n      }\r\n      .icon {\r\n        width: 0.2rem;\r\n        height: 0.2rem;\r\n        fill: #999;\r\n      }\r\n    }\r\n  }\r\n}\r\n.bgImg {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: absolute;\r\n  z-index: -1;\r\n  filter: blur(70px);\r\n}\r\n.detailContent {\r\n  width: 100%;\r\n  height: 9rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  position: relative;\r\n  .img_needle {\r\n    width: 2rem;\r\n    height: 3rem;\r\n    position: absolute;\r\n    left: 46%;\r\n    transform-origin: 0 0;\r\n    transform: rotate(-13deg);\r\n    transition: all 2s;\r\n  }\r\n  .img_needle-active {\r\n    width: 2rem;\r\n    height: 3rem;\r\n    position: absolute;\r\n    left: 46%;\r\n    transform-origin: 0 0;\r\n    transform: rotate(0deg);\r\n    transition: all 2s;\r\n  }\r\n\r\n  .img_needle_active {\r\n    width: 2rem;\r\n    height: 3rem;\r\n    position: absolute;\r\n    left: 46%;\r\n    transform-origin: 0 0;\r\n    transform: rotate(0deg);\r\n    transition: all 2s;\r\n  }\r\n  .img_cd {\r\n    width: 5rem;\r\n    height: 5rem;\r\n    position: absolute;\r\n    bottom: 2.3rem;\r\n    z-index: -1;\r\n  }\r\n  .img_ar {\r\n    width: 3.2rem;\r\n    height: 3.2rem;\r\n    border-radius: 50%;\r\n    position: absolute;\r\n    bottom: 3.14rem;\r\n    animation: rotate_ar 10s linear infinite;\r\n  }\r\n  .img_ar_active{\r\n    animation-play-state: running;\r\n  }\r\n  .img_ar_paused{\r\n    animation-play-state: paused;\r\n  }\r\n  @keyframes rotate_ar{\r\n    0%{\r\n      transform: rotateZ(0deg);\r\n    }\r\n    100%{\r\n      transform: rotateZ(360deg);\r\n    }\r\n  }\r\n}\r\n.detailFooter {\r\n  width: 100%;\r\n  height: 3rem;\r\n  position: absolute;\r\n  bottom: 0.2rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  .footerTop {\r\n    width: 100%;\r\n    height: 1rem;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n    .icon {\r\n      width: 0.36rem;\r\n      height: 0.36rem;\r\n      fill: rgb(245, 234, 234);\r\n    }\r\n    .icon {\r\n      width: 0.6rem;\r\n      height: 0.6rem;\r\n    }\r\n  }\r\n  .footerMid {\r\n    width: 100%;\r\n    height: 0.06rem;\r\n  }\r\n  .footerFoot {\r\n    width: 100%;\r\n    height: 1rem;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n    .icon {\r\n    //   fill: rgb(245, 234, 234);\r\n    fill: #999;\r\n    color: #999;\r\n    border-color: #999;\r\n    }\r\n    .bofang {\r\n      width: 1rem;\r\n      height: 1rem;\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AA6EA,SAASA,WAAU,QAAS,cAAc;AAC1C,OAAO,6BAA6B;AACpC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE,CAAC,WAAW,EAAC,WAAW,EAAC,kBAAkB,EAAC,MAAM,EAAC,YAAY,EAAC,WAAW,CAAC;EACnFC,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,SAAS,CAAC;EAC7B,CAAC;EACDC,UAAU,EAAE;IACVP;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,WAAW,EAAC;IACd;EACF,CAAC;EACDC,QAAQ,EAAC;IACPC,KAAK,EAAC,SAAAA,CAAA,EAAU;MACd,IAAIC,GAAG;MACP,IAAG,IAAI,CAACC,SAAS,CAACC,GAAG,CAACH,KAAK,EAAC;QAC1BC,GAAE,GAAI,IAAI,CAACC,SAAS,CAACC,GAAG,CAACH,KAAK,CAACI,KAAK,CAAC,eAAe,CAAC,CAACC,GAAG,CAAC,CAACC,IAAI,EAACC,CAAC,KAAG;UAClE,IAAIC,GAAE,GAAIF,IAAI,CAACG,KAAK,CAAC,CAAC,EAAC,CAAC,CAAC;UACzB,IAAIC,GAAE,GAAIJ,IAAI,CAACG,KAAK,CAAC,CAAC,EAAC,CAAC,CAAC;UACzB,IAAIE,IAAG,GAAIL,IAAI,CAACG,KAAK,CAACH,IAAI,CAACM,OAAO,CAAC,GAAG,CAAC,GAAC,CAAC,EAACN,IAAI,CAACM,OAAO,CAAC,GAAG,CAAC,CAAC;UAC5D,IAAIT,GAAE,GAAIG,IAAI,CAACG,KAAK,CAAC,EAAE,EAACH,IAAI,CAACO,MAAM,CAAC;UACpCpB,OAAO,CAACC,GAAG,CAACc,GAAG,EAACE,GAAG,EAACC,IAAI,EAACR,GAAG,CAAC;QAC/B,CAAC;MACH;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}