{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getPrivateMessageHistory, sendMessage } from \"@/request/api/home.js\";\nexport default {\n  name: \"privateMessageHistory\",\n  data() {\n    return {\n      uid: null,\n      historyMessages: [],\n      toUid: null,\n      inputText: \"\"\n    };\n  },\n  created: async function () {\n    this.uid = this.$route.query.uid;\n    this.toUid = this.$route.query.toUid;\n    var time = new Date();\n    this.historyMessages = (await getPrivateMessageHistory(this.uid, time)).data.msgs;\n    console.log(this.historyMessages);\n    for (var i = 0; i < this.historyMessages.length; i++) {\n      this.historyMessages[i].msg = JSON.parse(this.historyMessages[i].msg);\n    }\n    this.historyMessages.sort((a, b) => a.time - b.time);\n    console.log(this.toUid);\n    console.log(this.uid);\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.scrollToListEnd(); // 在下一个 Vue.js 更新周期中执行滚动操作\n    });\n  },\n\n  methods: {\n    sendMessage: async function () {\n      let result = await sendMessage(this.uid, this.inputText);\n      this.inputText = \"\";\n      for (var i = 0; i < result.data.newMsgs.length; i++) {\n        result.data.newMsgs[i].msg = JSON.parse(result.data.newMsgs[i].msg);\n        this.historyMessages.push(result.data.newMsgs[i]);\n      }\n      this.scrollToListEnd();\n    },\n    scrollToListEnd() {\n      const container = this.$refs.listContainer;\n      container.scrollTop = container.scrollHeight;\n    }\n  }\n};","map":{"version":3,"names":["getPrivateMessageHistory","sendMessage","name","data","uid","historyMessages","toUid","inputText","created","$route","query","time","Date","msgs","console","log","i","length","msg","JSON","parse","sort","a","b","mounted","$nextTick","scrollToListEnd","methods","result","newMsgs","push","container","$refs","listContainer","scrollTop","scrollHeight"],"sources":["D:\\java项目\\网易云音乐\\wangyiyun-app\\src\\views\\PrivateMessageHistory.vue"],"sourc